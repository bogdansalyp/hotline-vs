{
	"name": "Hotline VS",
	"type": "light",
	"colors": {
		"editor.background": "#151C25",
	},
	"tokenColors": [
	  {
		"name": "Comment",
		"scope": "comment",
		"settings": {
		  "foreground": "#8B21FF",
		  "fontStyle": "italic"
		}
	  },
	  {
		"name": "String",
		"scope": [
		  "string.quoted",
		  "string.template",
		  "punctuation.definition.string"
		],
		"settings": {
		  "foreground": "#23facb",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Variable",
		"scope": ["variable", "entity.name.variable", "variable.other.constant.property.js", "variable.other.property.js", "meta.object-literal.key", "support.type.property-name", "support.variable"],
		"settings": {
		  "foreground": "#f862bf",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Constant",
		"scope": "constant",
		"settings": {
		  "foreground": "#f963bf",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Parameter",
		"scope": "variable.parameter",
		"settings": {
		  "fontStyle": "italic bold",
		}
	  },
	  {
		"name": "Storage (declaration or modifier keyword)",
		"scope": ["storage.type", "storage.modifier"],
		"settings": {
		  "foreground": "#fa9b4e",
		  "fontStyle": "bold"
		}
	  },
	  
	  {
		"name": "Regex",
		"scope": "string.regexp",
		"settings": {
		  "foreground": "#24facb",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Number",
		"scope": "constant.numeric",
		"settings": {
		  "foreground": "#3CDe7e",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Language constant (boolean, null)",
		"scope": "constant.language",
		"settings": {
		  "foreground": "#ff6ffd",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Character escape",
		"scope": "constant.character.escape",
		"settings": {
		  "foreground": "#3cDe7f",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "HTML or XML tag",
		"scope": "entity.name.tag",
		"settings": {
		  "foreground": "#fe6ffb",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "HTML or XML tag brackets",
		"scope": ["punctuation.definition.tag"],
		"settings": {
		  "foreground": "#3bDe7f",
		  "fontStyle": "bold"
		}
	  },
	  
	  {
		"name": "Tag attribute HTML",
		"scope": "entity.other.attribute-name.html",
		"settings": {
		  "foreground": "#fb9b4e",
		  "fontStyle": "italic bold",
		}
	  },
	  {
		"name": "Class",
		"scope": ["entity.name.type", "meta.attribute.class.html"],
		"settings": {
		  "foreground": "#f761bf",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Inherited class",
		"scope": "entity.other.inherited-class",
		"settings": {
		  "foreground": "#ff6ffb",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Function",
		"scope": ["entity.name.function", "variable.function", "meta.function-call", "support.function"],
		"settings": {
		  "foreground": "#23faca",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "JS Export",
		"scope": ["keyword.control.export.js", "keyword.control.import.js"],
		"settings": {
		  "foreground": "#ff00fc",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Keyword",
		"scope": "keyword",
		"settings": {
		  "foreground": "#ff00fd",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Control keyword",
		"scope": "keyword.control",
		"settings": {
		  "foreground": "#ff00fe",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Operator",
		"scope": "keyword.operator",
		"settings": {
		  "foreground": "#3CDD7f",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Unit",
		"scope": "keyword.other.unit",
		"settings": {
		  "foreground": "#3CDD7e",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Support",
		"scope": "support",
		"settings": {
		  "foreground": "#f962bf",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Terminator",
		"scope": [
		  "punctuation.terminator"
		],
		"settings": {
		  "foreground": "#3CDD7d",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "CSS Selectt tag attribute",
		"scope": "entity.other.attribute-name.class.css",
		"settings": {
		  "foreground": "#24facc",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "CSS #id",
		"scope": "entity.other.attribute-name.id",
		"settings": {
		  "foreground": "#24faca",
		  "fontStyle": "bold"
		}
	  },
	  {
		"name": "Pseudo CSS",
		"scope": [
		  "entity.other.attribute-name.pseudo-element",
		  "entity.other.attribute-name.pseudo-class"
		],
		"settings": {
		  "foreground": "#fa9a4e",
		  "fontStyle": "bold"
		}
	  }
	]
}
  